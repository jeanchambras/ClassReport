{
  "name": "classreport",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chart.js": "^2.7.3",
    "image-capture": "^0.4.0",
    "react": "^16.6.3",
    "react-chartkick": "^0.3.0",
    "react-dom": "^16.6.3",
    "react-facebook-emoji": "^1.0.11",
    "react-scripts": "2.1.1",
    "request": "^2.88.0"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/index.sass src/index.css",
    "watch-css": "npm run css-build -- --watch",
    "start-js": "react-scripts start",
    "start": "touch src/index.css && npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bulma": "^0.7.2",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5"
  }
}
